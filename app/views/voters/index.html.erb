            <div class="btn-toolbar mb-2 mb-md-0">
            </div>
          </div>

          <h4 class="modal-title">Tech Team</h4>
          <div class="table-responsive">
            <table class="table table-striped table-sm">
            <thead>
              <tr>
                <th></th>
                <th>Name</th>
                <th class="td-actions">Praise or Criticize</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>/img</td>
                <td>Zarah</td>
                <td class="td-actions">
                    <button type="button" class="btn btn-success" data-toggle="modal" data-target="#praise">
                    <i class="btn-icon-only icon-ok"></i>
                    </button>
                  
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#criticize">
                    <i class="btn-icon-only icon-remove"></i>
                    </button>
                </td>
              </tr>
              <tr>
                <td>/img</td>
                <td>Alec</td>
                <td class="td-actions">
                    <button type="button" class="btn btn-success" data-toggle="modal" data-target="#praise">
                    <i class="btn-icon-only icon-ok"></i>
                    </button>
                  
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#criticize">
                    <i class="btn-icon-only icon-remove"></i>
                    </button>
                </td>
              </tr>
              <tr>
                <td>/img</td>
                <td>Chester</td>
                <td class="td-actions">
                    <button type="button" class="btn btn-success" data-toggle="modal" data-target="#praise">
                    <i class="btn-icon-only icon-ok"></i>
                    </button>
                  
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#criticize">
                    <i class="btn-icon-only icon-remove"></i>
                    </button>
                </td>
              </tr>
              <tr>
                <td>/img</td>
                <td>Drei</td>
                <td class="td-actions">
                    <button type="button" class="btn btn-success" data-toggle="modal" data-target="#praise">
                    <i class="btn-icon-only icon-ok"></i>
                    </button>
                  
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#criticize">
                    <i class="btn-icon-only icon-remove"></i>
                    </button>
                </td>
              </tr>
              <tr>
                <td>/img</td>
                <td>Angelo</td>
                <td class="td-actions">
                    <button type="button" class="btn btn-success" data-toggle="modal" data-target="#no-vote">
                    <i class="btn-icon-only icon-ok"></i>
                    </button>
                  
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#no-vote">
                    <i class="btn-icon-only icon-remove"></i>
                    </button>
                </td>
              </tr>
              </tbody>
            </table>
          </div>
          <canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>
        </main>
      </div>
    </div>

